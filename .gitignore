#include <stdio.h>
int main(){
    
    char ch;
    printf("Enter your letter: ");
    scanf("%c", &ch);

  
    if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u'){
        printf("%c is a lower case Vowel.\n", ch);
    }
    else if (ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U'){
        printf("%c is a Upper case Voerl.\n", ch);
    }
    else if (ch >= 'A' && ch <= 'Z'){
        printf("%c is a Upper case consonat.\n", ch);
    }
    else if(ch >= 'a' && ch <= 'z'){
      printf("%c is a lower case consonat.\n", ch);
    }
}
